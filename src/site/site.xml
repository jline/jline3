<project xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

    <bannerLeft>
        <name>jline</name>
        <src>http://jline.github.com/jline2/images/jline.jpg</src>
        <href>http://jline.github.com</href>
    </bannerLeft>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>

    <publishDate position="right"/>
    <version position="right"/>

    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <gitHub>
                <projectId>jline/jline2</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>darkblue</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>

    <body>
        <breadcrumbs>
            <item name="${project.name}" href="${project.url}/index.html"/>
        </breadcrumbs>

        <menu name="Overview">
            <item name="About" href="index.html"/>
        </menu>

        <menu ref="modules"/>

        <menu ref="reports"/>
    </body>
</project>